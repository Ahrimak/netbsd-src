<section class="Sh">
<h1 class="Sh" id="NAME"><a class="permalink" href="#NAME">NAME</a></h1>
<code class="Nm">arp</code> &#x2014;
<div class="Nd">address resolution display and control</div>
</section>
<section class="Sh">
<h1 class="Sh" id="SYNOPSIS"><a class="permalink" href="#SYNOPSIS">SYNOPSIS</a></h1>
<table class="Nm">
  <tr>
    <td><code class="Nm">arp</code></td>
    <td>[<code class="Fl">-n</code>] <var class="Ar">hostname</var></td>
  </tr>
</table>
<br/>
<table class="Nm">
  <tr>
    <td><code class="Nm">arp</code></td>
    <td>[<code class="Fl">-nv</code>] <code class="Fl">-a</code></td>
  </tr>
</table>
<br/>
<table class="Nm">
  <tr>
    <td><code class="Nm">arp</code></td>
    <td>[<code class="Fl">-v</code>] <code class="Fl">-d</code>
      <code class="Fl">-a</code></td>
  </tr>
</table>
<br/>
<table class="Nm">
  <tr>
    <td><code class="Nm">arp</code></td>
    <td>[<code class="Fl">-v</code>] <code class="Fl">-d</code>
      <var class="Ar">hostname</var> [<code class="Cm">proxy</code>]</td>
  </tr>
</table>
<br/>
<table class="Nm">
  <tr>
    <td><code class="Nm">arp</code></td>
    <td><code class="Fl">-s</code> <var class="Ar">hostname ether_addr</var>
