<section class="Sh">
<h1 class="Sh" id="NAME"><a class="permalink" href="#NAME">NAME</a></h1>
<code class="Nm">mopprobe</code> &#x2014;
<div class="Nd">MOP Probe Utility</div>
</section>
<section class="Sh">
<h1 class="Sh" id="SYNOPSIS"><a class="permalink" href="#SYNOPSIS">SYNOPSIS</a></h1>
<table class="Nm">
  <tr>
    <td><code class="Nm">mopprobe</code></td>
    <td><code class="Fl">-a</code> [<code class="Fl">-3</code> |
      <code class="Fl">-4</code>]</td>
  </tr>
</table>
<br/>
<table class="Nm">
  <tr>
    <td><code class="Nm">mopprobe</code></td>
    <td>[<code class="Fl">-3</code> | <code class="Fl">-4</code>]
