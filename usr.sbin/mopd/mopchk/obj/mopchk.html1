<section class="Sh">
<h1 class="Sh" id="NAME"><a class="permalink" href="#NAME">NAME</a></h1>
<code class="Nm">mopchk</code> &#x2014;
<div class="Nd">MOP Check Utility</div>
</section>
<section class="Sh">
<h1 class="Sh" id="SYNOPSIS"><a class="permalink" href="#SYNOPSIS">SYNOPSIS</a></h1>
<table class="Nm">
  <tr>
    <td><code class="Nm">mopchk</code></td>
    <td>[<code class="Fl">-a</code>] [<code class="Fl">-v</code>]
      [<var class="Ar">filename</var>]</td>
  </tr>
